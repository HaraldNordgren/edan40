

match :: Eq a => a -> [a] -> [a] -> Maybe [a]
match _ [] [] = Just []
match _ [] _ = Nothing
match _ _ [] = Nothing
match wildcard (p:ps) (s:ss)
    | (p == wildcard) = orElse(singlewildcar, longerwildcard)
    | p == s = match wildcard ps ss
    | otherwise = Nothing

